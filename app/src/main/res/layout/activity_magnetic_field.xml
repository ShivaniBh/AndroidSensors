<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:id="@+id/relative3"
    tools:context="com.example.user1.AssignmentOne810002540.MagneticFieldActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/name2"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text=""
        android:id="@+id/xval2"
        android:layout_below="@+id/name2"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text=""
        android:id="@+id/yval2"
        android:layout_below="@+id/xval2"
        android:layout_alignLeft="@+id/zval2"
        android:layout_alignStart="@+id/zval2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text=""
        android:id="@+id/zval2"
        android:layout_below="@+id/yval2"
        android:layout_alignLeft="@+id/xval2"
        android:layout_alignStart="@+id/xval2" />

    <Button
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:text="START"
        android:id="@+id/magneticFieldOn"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:text="STOP"
        android:id="@+id/magneticFieldOff"
        android:layout_below="@+id/magneticFieldOn"
        android:layout_alignLeft="@+id/magneticFieldOn"
        android:layout_alignStart="@+id/magneticFieldOn" />

</RelativeLayout>
